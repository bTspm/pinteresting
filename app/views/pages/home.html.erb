<div class="jumbotron center">
  <center>
      <h1>Welcome</h1>
        <p>
          <% if user_signed_in? %>
          <h4> You are already Signed In </h>
            <%=link_to "Log Out", destroy_user_session_path, method: :delete %>
          <% else %>
            <%=link_to "Sign Up", '/users/sign_up', class: "btn btn-primary " %>
            <%=link_to "Log In", '/users/sign_in', class: "btn btn-default" %>
          <% end %>
        </p>
  </center>
</div>
